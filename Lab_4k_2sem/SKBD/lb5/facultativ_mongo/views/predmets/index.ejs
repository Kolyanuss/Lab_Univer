<% include ../partials/header %>

<h1>Predmets List</h1>

<a href="/predmets/create">Create a New Predmet</a>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% predmets.forEach(function(predmet) { %>
      <tr>
        <td><%= predmet.name %></td>
        <td>
          <a href="/predmets/<%= predmet._id %>">Show</a>
          <a href="/predmets/edit/<%= predmet._id %>">Edit</a>
          <form method="POST" action="/predmets/<%= predmet._id %>?_method=DELETE" style="display:inline">
            <button type="submit">Delete</button>
          </form>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>

<% include ../partials/footer %> -->