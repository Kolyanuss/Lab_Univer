<%- include ("../partials/header")%>

<h1>Create a New Predmet</h1>

<form method="POST" action="/predmets">
  <div>
    <label for="name">Name</label>
    <input type="text" id="name" name="name" required>

    <label for="lecturesize">lecturesize</label>
    <input type="number" id="lecturesize" name="lecturesize" required>

    <label for="practicesize">practicesize</label>
    <input type="number" id="practicesize" name="practicesize" required>
    
    <label for="labsize">labsize</label>
    <input type="number" id="labsize" name="labsize" required>
  </div>
  <button type="submit">Create</button>
</form>

<%- include ("../partials/footer") %>